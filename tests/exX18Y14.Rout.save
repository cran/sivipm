> 
> datancol <- ncol(X18Y14)
> nX=18
> X = X18Y14[,1:nX]
> Y = X18Y14[,(nX+1):datancol]
> # -------------------------------------
> # CALCULATIONS WITH POLYNOME DESCRIPTION NOT REQUIRED
> nc=2
> b <- new("polyX", dataX.exp=X)
> print( sivipm(Y, b, nc=nc))
